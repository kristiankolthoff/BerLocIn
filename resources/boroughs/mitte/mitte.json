    { "area": { "type": "typed-literal", "datatype": "http://www.w3.org/2001/XMLSchema#double", "value": "1.07e+07" }	, "abstract": { "type": "literal", "xml:lang": "en", "value": "Mitte (German for \"middle, centre\", commonly used without an article) is a central locality (Ortsteil) of Berlin in the homonymous district (Bezirk) of Mitte. Until 2001 it was itself an autonomous district. It comprises the historic centre of Alt-Berlin around the churches of St. Nicholas and St. Mary, renowned Museum Island, the city hall Rotes Rathaus, the city administrative building Altes Stadthaus, the famous Fernsehturm, Brandenburg Gate at the end of the central boulevard Unter den Linden and more main tourist attractions of the city. For these reasons Mitte is considered the \"heart\" of Berlin." }	, "district": { "type": "uri", "value": "http://dbpedia.org/resource/Mitte" }	, "areaCode": { "type": "literal", "value": "B" }	, "elevation": { "type": "typed-literal", "datatype": "http://www.w3.org/2001/XMLSchema#double", "value": "52.0" }	, "populationAsOf": { "type": "typed-literal", "datatype": "http://www.w3.org/2001/XMLSchema#date", "value": "2009-06-30" }	, "populationTotal": { "type": "typed-literal", "datatype": "http://www.w3.org/2001/XMLSchema#nonNegativeInteger", "value": "79582" }	, "postalCode": { "type": "literal", "value": "(nr. 0101) 10115, 10117, 10119, 10178, 10179" }	, "density": { "type": "typed-literal", "datatype": "http://www.w3.org/2001/XMLSchema#integer", "value": "7445" }	, "latDeg": { "type": "typed-literal", "datatype": "http://www.w3.org/2001/XMLSchema#integer", "value": "52" }	, "latMin": { "type": "typed-literal", "datatype": "http://www.w3.org/2001/XMLSchema#integer", "value": "31" }	, "latSec": { "type": "typed-literal", "datatype": "http://www.w3.org/2001/XMLSchema#integer", "value": "10" }	, "lonDeg": { "type": "typed-literal", "datatype": "http://www.w3.org/2001/XMLSchema#integer", "value": "13" }	, "lonMin": { "type": "typed-literal", "datatype